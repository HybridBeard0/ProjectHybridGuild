		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.rich2";
			Text = "Dem einfachen Volk fehlen die geistigen Fähigkeiten, um Lesen und Schreiben zu lernen. Denkt Ihr nicht auch?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.poor1";
