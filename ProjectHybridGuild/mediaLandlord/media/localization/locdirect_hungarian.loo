		{
			Key = "$ui.Subquest.Desc15";
			Text = "Keress érméket a távolsági kereskedelmi útvonalakon keresztül.";
		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc16";
			Text = "Küldd el az avatárodat vagy egy családtagodat egy hosszútávú utazásra.";
		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc16.1";
			Text = "Küldd el egy családtagodat egy hosszútávú utazásra.";
		},
