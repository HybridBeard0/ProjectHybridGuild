		{
			Key = "$action.GetGastronomyService";
			Text = "Látogass el egy fogadóba";
		},
		LocDirectEntry
		{
			Key = "$action.GetGastronomyServiceDesc";
			Text = "Tölts el egy kis időt egy fogadóban, igyál egy kis sört vagy egyél egy finom vacsorát és közben hallgasd meg a legújabb pletykákat.";
		},
