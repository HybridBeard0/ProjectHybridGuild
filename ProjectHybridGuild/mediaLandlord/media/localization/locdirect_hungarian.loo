			Text = "Találd meg a harmadik áruló dinasztiát, mielőtt megszereznéd a \"lord\" címet.";
		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc12";
			Text = "Az összes hivatal egyszerre legyen a legmagasabb szinten egy városban.";
		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc13";
			Text = "Szerezd meg a Földesúr címet";
		},
		LocDirectEntry
