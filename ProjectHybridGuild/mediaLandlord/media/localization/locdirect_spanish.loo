		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.poor1";
			Text = "Tendrías que cambiar tu catre de paja, ahí es donde viven los bichos. Lo mejor es que lo quemes.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.poor2";
			Text = "¿Eso de tu brazo es una pulga? ¡Ay, no! ¡La tengo encima! ¡Aléjate de mí!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.com1";
			Text = "Dicen que el incienso sirve contra los bichos. ¿Tú lo has probado?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.com2";
			Text = "Tenía ratas en casa, pero el hijo del vecino las espantó... y luego asaltó mi cocina...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.rich1";
			Text = "Bueno, tendré que ir al barbero y frotarme bien en la bañera.";
