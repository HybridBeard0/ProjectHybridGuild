		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc14";
			Text = "Alcanza el nivel mínimo de popularidad en dos distritos de tu ciudad.";
		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc15";
			Text = "Gana un determinado número de monedas gracias a las rutas comerciales de larga distancia.";
		},
