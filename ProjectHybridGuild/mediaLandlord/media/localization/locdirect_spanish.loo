			Key = "$ui.Subquest.Desc16";
			Text = "Envía a tu avatar o a un miembro de la familia a un largo viaje.";
		},
		LocDirectEntry
		{
			Key = "$ui.Subquest.Desc16.1";
			Text = "Envía a un miembro de la familia a un largo viaje.";
		},
		LocDirectEntry
