		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.poor1";
			Text = "Mi piacerebbe saper leggere e scrivere...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.poor2";
			Text = "Mi piacerebbe sapere cosa scrivono gli eruditi sulle loro pergamene. Bisognerebbe saper leggere.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.com1";
			Text = "Sto leggendo un libro da anni. Sono già a pagina 11... Se solo sapessi leggere meglio.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.com2";
			Text = "Se sapessi leggere e scrivere, terrei un diario. Così i posteri saprebbero di tutte le mie avventure.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.rich1";
			Text = "Penso che dovrei iniziare a imparare a leggere e scrivere in latino.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.line.rich2";
			Text = "Alla gente comune mancano le facoltà mentali per imparare a leggere e scrivere. Non credete?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.poor1";
			Text = "Pensateci. Leggere e scrivere vi aiuterebbe nel lavoro manuale?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.poor2";
			Text = "Non riesco a immaginare che si possa imparare qualcosa di utile leggendo scarabocchi.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.com1";
			Text = "Sarebbe anche interessante sapere cosa c'è scritto in tutti questi libri.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.com2";
			Text = "A che serve leggere, eh? Non ho mai sentito di nessuno che metta per iscritto le istruzioni di lavoro.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.rich1";
			Text = "Leggere e scrivere dovrebbe essere consentito solo all'alta società, per legge.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.literacy.answer.rich2";
			Text = "Per imparare a leggere e scrivere bisogna entrare in convento. Oppure pagare un sacco di soldi per un insegnante...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.line.poor1";
			Text = "Ho morsi di parassiti su tutto il corpo... Che fastidio...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.line.poor2";
			Text = "Recentemente un ratto ha pensato bene di mordermi. Probabilmente pensava che fossi pancetta o formaggio...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.line.com1";
			Text = "Oggi ho trovato un ratto morto nella mia scorta di grano. Avrà mangiato fino a morirne, mentre io devo morire di fame...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.line.com2";
			Text = "Parassiti immondi! Ho prurito dappertutto. Forse dovrei dare una bella scrollata ai miei vestiti...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.line.rich1";
			Text = "Dovreste applicare questa polvere sulla pelle. Lenisce magnificamente il fastidio di pustole e morsi di parassiti.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.line.rich2";
			Text = "Verso acqua di rose sulle lenzuola ogni sera prima di andare a letto. Pare che protegga dai parassiti.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.poor1";
			Text = "Dovreste cambiare il pagliericcio. È lì che si annidano le creature. Meglio bruciarlo subito.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.poor2";
			Text = "Avete una pulce sul braccio... Oh no, adesso è saltata su di me! Statemi alla larga!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.com1";
			Text = "Si dice che l'incenso aiuti contro i parassiti. Avete mai provato?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.com2";
			Text = "Avevo dei ratti a casa, ma il figlio dei vicini li ha cacciati via. E poi ha razziato la mia cucina...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.rich1";
			Text = "Beh, preferirei andare dal barbiere e lavarmi per bene nella vasca.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.hygiene.answer.rich2";
			Text = "Bisogna tenersi a debita distanza dai plebei. È da lì che vengono i parassiti.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.line.poor1";
			Text = "Fa così freddo... e non possiamo permetterci legna da ardere...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.line.poor2";
			Text = "Ho finito la legna da ardere... state gelando anche voi?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.line.com1";
			Text = "Si è messo a far freddo e non ho legna da ardere. Spero di non buscarmi un malanno.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.line.com2";
			Text = "Quei ricchi mercanti non si congeleranno certo, nelle loro costose pellicce... Mamma mia, che freddo...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.line.rich1";
			Text = "I miei servi non hanno procurato abbastanza legna da ardere. Ora non possono riscaldare a sufficienza la mia sala da pranzo.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.line.rich2";
			Text = "La mia serva ha scaldato troppo la pietra scaldaletto. Che fastidio!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.answer.poor1";
			Text = "Sì, avete perfettamente ragione...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.answer.poor2";
			Text = "Uhm... ehm... davvero terribile...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.answer.com1";
			Text = "Avete tutta la mia comprensione.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.answer.com2";
			Text = "Suvvia, state esagerando.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.answer.rich1";
			Text = "Inaudito! Dovreste fare qualcosa a riguardo!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.firewood.answer.rich2";
			Text = "Scandaloso! Fosse per me, non accetterei mai una cosa del genere e prenderei subito provvedimenti!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.line.poor1";
			Text = "Credo ci sia qualcuno nascosto là dietro...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.line.poor2";
			Text = "Avete sentito anche voi? C'era qualcosa che si muoveva, nell'ombra laggiù...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.line.com1";
			Text = "Laggiù! Avete visto? Quei furfanti si sono fatti di nuovo dei cenni segreti con la mano!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.line.com2";
			Text = "Credo proprio che qualcuno mi stia seguendo.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.line.rich1";
			Text = "Ho sentito qualcuno sguainare un pugnale dietro di me. Ma quando ho guardato non c'era nessuno...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.line.rich2";
			Text = "Non dovremmo andare in giro a quest'ora. Potrebbero esserci ladri in agguato ovunque...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.answer.poor1";
			Text = "Non ho niente che valga la pena rubare. Non ho paura.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.answer.poor2";
			Text = "Sì, ci sono di nuovo parecchi furfanti in giro...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.answer.com1";
			Text = "Oh, state nuovamente vedendo i fantasmi...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.answer.com2";
			Text = "Ne avete la certezza? Allora faremmo meglio a nascondere le nostre borse... Dov'è la mia borsa?!?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.answer.rich1";
			Text = "Se c'è qualcuno, di certo non oserà mettersi contro la mia famiglia.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.shadows.answer.rich2";
			Text = "Restate qui e distraeteli... voglio dire, tenete gli occhi aperti. Io vado a cercare la guardia cittadina.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.line.poor1";
			Text = "Avete sentito anche voi?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.line.poor2";
			Text = "Ho un brutto presentimento. Anche voi?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.line.com1";
			Text = "Mi prude il naso... Sta per succedere qualcosa di brutto...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.line.com2";
			Text = "Ascoltatemi: i demoni si nascondono nell'ombra! Bramano le anime dei giusti!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.line.rich1";
			Text = "Questo parlare di demoni spaventa solo i deboli di mente. Ma... non capisco... davvero credete che esistano i demoni?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.line.rich2";
			Text = "Mi sento un po' a disagio qui. La gente è talmente empia che il diavolo stesso non può essere lontano...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.answer.poor1";
			Text = "Oh, non c'è niente lì.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.answer.poor2";
			Text = "Ehm... sì! Dovremmo... ehm... dov'è la mia croce protettiva?!?";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.answer.com1";
			Text = "Non temete. Vi proteggerò io!";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.answer.com2";
			Text = "Oh no! Stanno venendo a prenderci...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.answer.rich1";
			Text = "No, non credo. Siamo al sicuro qui. Dio è con noi... o almeno, è con me.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.ghosts.answer.rich2";
			Text = "Ho qui il mio pugnale. L'ho fatto benedire proprio ieri...";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.SovereignDead.line.poor1.IsMale";
			Text = "Sua Maestà %CharacterContext% è passato a miglior vita.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.SovereignDead.line.poor1.IsFemale";
			Text = "Sua Maestà %CharacterContext% è passata a miglior vita.";
		},
		LocDirectEntry
		{
			Key = "$d.npcChat.SovereignDead.line.poor2.IsMale";
			Text = "Sua Maestà %CharacterContext% non c'è più...";
		},
